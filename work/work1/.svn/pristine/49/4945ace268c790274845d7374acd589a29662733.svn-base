<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/bg_znfz_normal"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/d150"
        android:background="@drawable/bg_home_head">


        <ImageView
            android:id="@+id/applicant_my_iv_head"
            android:layout_width="@dimen/d60"
            android:layout_height="@dimen/d60"
            android:background="@drawable/ic_user_head_default"
            android:layout_marginTop="@dimen/d30"
            android:layout_centerHorizontal="true"/>

        <TextView
            android:id="@+id/applicant_my_tv_username"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/applicant_my_nousername"
            android:textSize="@dimen/d20"
            android:layout_below="@+id/applicant_my_iv_head"
            android:textColor="@color/white"
            android:alpha="0.7"
            android:layout_marginTop="@dimen/d10"
            android:layout_centerHorizontal="true"/>

        <Button
            android:id="@+id/applicant_my_btn_changerole"
            android:text="切换发布"
            android:padding="@dimen/d2"
            android:layout_width="@dimen/d80"
            android:layout_height="30dp"
            android:textSize="@dimen/d12"
            android:textColor="@color/white"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:background="@drawable/bg_login_phone"/>

    </RelativeLayout>

    <!-- 我的钱包  -->
    <RelativeLayout
        android:id="@+id/applicant_my_wallet_rl"
        android:layout_width="match_parent"
        android:layout_height="@dimen/d50"
        android:layout_marginLeft="@dimen/d20"
        android:layout_marginRight="@dimen/d20"
        android:layout_marginTop="@dimen/d10"
        android:background="@color/white"
        android:gravity="center_vertical"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/applicant_my_wallet_rl_iv"
            android:layout_width="@dimen/d30"
            android:layout_height="@dimen/d30"
            android:layout_marginLeft="@dimen/d15"
            android:background="@mipmap/my_wallet"
            android:layout_centerVertical="true"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="@string/applicant_my_mywallet"
            android:textSize="@dimen/d18"
            android:layout_toRightOf="@+id/applicant_my_wallet_rl_iv"
            android:textColor="@color/black"
            android:gravity="center"
            android:layout_marginLeft="@dimen/d15"
            />

        <TextView
            android:id="@+id/aapplicant_my_wallet_rl_tv_money"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="500.00"
            android:textSize="@dimen/d18"
            android:textColor="@color/red"
            android:layout_alignParentRight="true"
            android:gravity="center"
            android:layout_marginRight="@dimen/d15"
            />

    </RelativeLayout>

    <!-- 积分纪录  -->
    <RelativeLayout
        android:id="@+id/applicant_income_rl"
        android:layout_width="match_parent"
        android:layout_height="@dimen/d50"
        android:layout_marginLeft="@dimen/d20"
        android:layout_marginRight="@dimen/d20"
        android:layout_marginTop="@dimen/d10"
        android:background="@color/white"
        android:gravity="center_vertical"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/applicant_income_rl_iv"
            android:layout_width="@dimen/d30"
            android:layout_height="@dimen/d30"
            android:layout_marginLeft="@dimen/d15"
            android:background="@mipmap/my_integral"
            android:layout_centerVertical="true"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="@string/applicant_my_incomerecord"
            android:textSize="@dimen/d18"
            android:layout_toRightOf="@+id/applicant_income_rl_iv"
            android:textColor="@color/black"
            android:gravity="center"
            android:layout_marginLeft="@dimen/d15"
            />

        <ImageView
            android:id="@+id/applicant_income_rl_iv_arrow"
            android:layout_width="10dp"
            android:layout_height="match_parent"
            android:layout_marginTop="@dimen/d15"
            android:layout_marginBottom="@dimen/d15"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/d15"
            android:background="@drawable/ic_arrow_right"
            />

    </RelativeLayout>

    <!-- 我的发布  -->
    <RelativeLayout
        android:id="@+id/applicant_mypublish_rl"
        android:layout_width="match_parent"
        android:layout_height="@dimen/d50"
        android:layout_marginLeft="@dimen/d20"
        android:layout_marginRight="@dimen/d20"
        android:layout_marginTop="@dimen/d10"
        android:background="@color/white"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:visibility="gone">

        <ImageView
            android:id="@+id/applicant_mypublish_rl_iv"
            android:layout_width="@dimen/d30"
            android:layout_height="@dimen/d30"
            android:layout_marginLeft="@dimen/d15"
            android:background="@mipmap/my_info"
            android:layout_centerVertical="true"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="@string/applicant_my_publish"
            android:textSize="@dimen/d18"
            android:layout_toRightOf="@+id/applicant_mypublish_rl_iv"
            android:textColor="@color/black"
            android:gravity="center"
            android:layout_marginLeft="@dimen/d15"
            />

        <ImageView
            android:id="@+id/applicant_mypublish_rl_iv_arrow"
            android:layout_width="10dp"
            android:layout_height="match_parent"
            android:layout_marginTop="@dimen/d15"
            android:layout_marginBottom="@dimen/d15"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/d15"
            android:background="@drawable/ic_arrow_right"
            />

    </RelativeLayout>

    <!-- 我的资料  -->
    <RelativeLayout
        android:id="@+id/applicant_myinfo_rl"
        android:layout_width="match_parent"
        android:layout_height="@dimen/d50"
        android:layout_marginLeft="@dimen/d20"
        android:layout_marginRight="@dimen/d20"
        android:layout_marginTop="@dimen/d10"
        android:background="@color/white"
        android:gravity="center_vertical"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/applicant_myinfo_rl_iv"
            android:layout_width="@dimen/d30"
            android:layout_height="@dimen/d30"
            android:layout_marginLeft="@dimen/d15"
            android:background="@mipmap/my_info"
            android:layout_centerVertical="true"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="@string/applicant_my_myinfo"
            android:textSize="@dimen/d18"
            android:layout_toRightOf="@+id/applicant_myinfo_rl_iv"
            android:textColor="@color/black"
            android:gravity="center"
            android:layout_marginLeft="@dimen/d15"
            />

        <ImageView
            android:id="@+id/applicant_myinfo_rl_iv_arrow"
            android:layout_width="10dp"
            android:layout_height="match_parent"
            android:layout_marginTop="@dimen/d15"
            android:layout_marginBottom="@dimen/d15"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/d15"
            android:background="@drawable/ic_arrow_right"
            />

    </RelativeLayout>

    <!-- 我的好友  -->
    <RelativeLayout
        android:id="@+id/applicant_myfriend_rl"
        android:layout_width="match_parent"
        android:layout_height="@dimen/d50"
        android:layout_marginLeft="@dimen/d20"
        android:layout_marginRight="@dimen/d20"
        android:layout_marginTop="@dimen/d10"
        android:background="@color/white"
        android:gravity="center_vertical"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/applicant_myfriend_rl_iv"
            android:layout_width="@dimen/d30"
            android:layout_height="@dimen/d30"
            android:layout_marginLeft="@dimen/d15"
            android:background="@mipmap/my_friend"
            android:layout_centerVertical="true"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="@string/applicant_my_myfriend"
            android:textSize="@dimen/d18"
            android:layout_toRightOf="@+id/applicant_myfriend_rl_iv"
            android:textColor="@color/black"
            android:gravity="center"
            android:layout_marginLeft="@dimen/d15"
            />

        <ImageView
            android:id="@+id/applicant_myfriend_rl_iv_arrow"
            android:layout_width="10dp"
            android:layout_height="match_parent"
            android:layout_marginTop="@dimen/d15"
            android:layout_marginBottom="@dimen/d15"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/d15"
            android:background="@drawable/ic_arrow_right"
            />

    </RelativeLayout>

    <!-- 我的系统身份  -->
    <RelativeLayout
        android:id="@+id/applicant_mysystemid_rl"
        android:layout_width="match_parent"
        android:layout_height="@dimen/d50"
        android:layout_marginLeft="@dimen/d20"
        android:layout_marginRight="@dimen/d20"
        android:layout_marginTop="@dimen/d10"
        android:background="@color/white"
        android:gravity="center_vertical"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/applicant_mysystemid_rl_iv"
            android:layout_width="@dimen/d30"
            android:layout_height="@dimen/d30"
            android:layout_marginLeft="@dimen/d15"
            android:background="@mipmap/my_system_identity"
            android:layout_centerVertical="true"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="@string/applicant_my_mysystemid"
            android:textSize="@dimen/d18"
            android:layout_toRightOf="@+id/applicant_mysystemid_rl_iv"
            android:textColor="@color/black"
            android:gravity="center"
            android:layout_marginLeft="@dimen/d15"
            />

        <ImageView
            android:id="@+id/applicant_mysystemid_rl_iv_arrow"
            android:layout_width="10dp"
            android:layout_height="match_parent"
            android:layout_marginTop="@dimen/d15"
            android:layout_marginBottom="@dimen/d15"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/d15"
            android:background="@drawable/ic_arrow_right"
            />

    </RelativeLayout>





</LinearLayout>